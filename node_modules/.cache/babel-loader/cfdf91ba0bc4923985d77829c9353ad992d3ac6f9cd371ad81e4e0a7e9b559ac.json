avigate = useNavigate();\r\n\r\n    useEffect(() => {\r\n        const authUserString = localStorage.getItem('auth');\r\n        if (authUserString) {\r\n            setIsAuth(true);\r\n        } else {\r\n            setIsAuth(false);\r\n        }\r\n    }, [navigate]);\r\n\r\n    const handleLogout = () => {\r\n        localStorage.removeItem('auth');\r\n        setIsAuth(false);\r\n        navigate('/login');\r\n    };\r\n\r\n    return (\r\n        <nav>\r\n            <div className=\"nav-links\">\r\n                <Link className=\"nav-link\" to=\"/\">Home</Link>\r\n                {isAuth && <Link className=\"nav-link\" to=\"/dashboard\">Dashboard</Link>}\r\n                <Link className=\"nav-link\" to=\"/about\">About</Link>\r\n                <Link className=\"nav-link\" to=\"/contact\">Contact</Link>\r\n            </div>\r\n            <div className=\"nav-actions\">\r\n                {isAuth ? (\r\n                    <button onClick={handleLogout}>Log Out</button>\r\n                ) : (\r\n                    <>\r\n                        <Link className=\"nav-action\" to=\"/login\">Log In</Link>\r\n                        <Link className=\"nav-action\" to=\"/signup\">Sign Up</Link>\r\n                    </>\r\n                )}\r\n            </div>\r\n        </nav>\r\n    );\r\n};\r\n"],"mappings":"AAAA,MAAO,sBAAsB,CAC7B,OAASA,IAAI,CAAEC,WAAW,KAAQ,kBAAkB,CACpD,OAASC,SAAS,CAAEC,QAAQ,KAAQ,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,CAAAC,QAAA,IAAAC,SAAA,yBAE5C,MAAO,MAAM,CAAAC,MAAM,CAAGA,CAAA,GAAM,CACxB,KAAM,CAACC,MAAM,CAAEC,SAAS,CAAC,CAAGT,QAAQ,CAAU,KAAK,CAAC,CACpD,KAAM,CAAAU,QAAQ,CAAGZ,WAAW,CAAC,CAAC,CAE9BC,SAAS,CAAC,IAAM,CACZ,KAAM,CAAAY,cAAc,CAAGC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CACnD,GAAIF,cAAc,CAAE,CAChBF,SAAS,CAAC,IAAI,CAAC,CACnB,CAAC,IAAM,CACHA,SAAS,CAAC,KAAK,CAAC,CACpB,CACJ,CAAC,CAAE,CAACC,QAAQ,CAAC,CAAC,CAEd,KAAM,CAAAI,YAAY,CAAGA,CAAA,GAAM,CACvBF,YAAY,CAACG,UAAU,CAAC,MAAM,CAAC,CAC/BN,SAAS,CAAC,KAAK,CAAC,CAChBC,QAAQ,CAAC,QAAQ,C